{
  "$schema": "../node_modules/@tauri-apps/cli/config.schema.json",
  "productName": "NET4Y",
  "version": "0.1.0",
  "identifier": "com.tauri.build",
  "build": {
    "frontendDist": "../dist",
    "devUrl": "http://192.168.56.1:3000",
    "beforeDevCommand": "npm run dev",
    "beforeBuildCommand": "npm run build"
  },
  "app": {
    "windows": [
      {
        "title": "NET4Y",
        "width": 1920,
        "height": 1080,
        "resizable": true,
        "fullscreen": false
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  },
  "plugins": {
		"fs": {
			"scope": {
				"allow": [
					"http://192.168.1.24:8090/*",
          "http://192.168.1.24:9000/*"
				]
			}
		}
	}
}
